{
  "name": "jinja2-html-enhancer",
  "displayName": "Jinja2 HTML Enhancer",
  "description": "Format and syntax highlighting for Jinja2 templates with .jinja2.html extension",
  "version": "0.0.3",
  "publisher": "Xubylele",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Formatters"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "html",
        "extensions": [
          ".jinja2.html"
        ],
        "aliases": [
          "HTML (Jinja2)"
        ]
      }
    ],
    "grammars": [
      {
        "language": "html",
        "scopeName": "text.html.jinja2",
        "path": "./syntaxes/jinja2.tmLanguage.json",
        "injectTo": [
          "text.html"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.formatJinja2",
        "title": "Format Jinja2 HTML File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}